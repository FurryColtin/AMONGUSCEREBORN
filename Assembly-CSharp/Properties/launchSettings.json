{
  "profiles": {
    "Start": {
      "commandName": "Executable",
      "executablePath": "$(SolutionDir)\\Build\\LaunchCE.bat",
      "commandLineArgs": "$(SolutionDir)"
    },
    "Publish": {
      "commandName": "Executable",
      "executablePath": "$(SolutionDir)\\Build\\PublishCE.bat",
      "commandLineArgs": "\"$(TargetDir)\"",
      "workingDirectory": "$(SolutionDir)"
    },
    "Start (Multiple)": {
      "commandName": "Executable",
      "executablePath": "$(SolutionDir)\\Build\\MultiLaunchCE.bat",
      "commandLineArgs": "$(SolutionDir)"
    }
  }
}